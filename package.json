  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@mantine/core": "^6.0.7",
    "@mantine/form": "^6.0.8",
    "@mantine/hooks": "^6.0.7",
    "@mantine/notifications": "^6.0.2",
    "@nem035/gpt-3-encoder": "^1.1.7",
    "@tabler/icons-react": "^2.15.0",
    "axios": "^1.3.5",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "eventemitter3": "^5.0.0",
    "eventsource-parser": "^1.0.0",
    "is-mobile": "^4.0.0",
    "js-audio-recorder": "^1.0.7",
    "next": "13.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.7",
    "rehype-katex": "^6.0.2",
    "rehype-prism-plus": "^1.5.1",
    "remark-breaks": "^3.0.2",
