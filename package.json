{
  "name": "nova-gpt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "proxy-dev": "proxychains4 -f ./proxychains.conf pnpm dev",
    "prepare": "husky install",
